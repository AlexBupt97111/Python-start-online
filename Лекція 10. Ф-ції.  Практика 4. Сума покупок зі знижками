def create_good(item, price):#to create good
    good = {
      "description": item,
      "price": price  
    }#create good (dict with name and price of certain item)
    return good #return done dict

def calc_new_price(good, sk):#define new price
    percent = 0
    for price in sk.keys():#return keys (500,1000)
        if good.get("price") > price: #good price > price
            percent = sk.get(price)#get value(5,7)
    discount = (good.get("price")*percent)//100
    new_price = good.get("price") - discount
    return new_price

def calculate_buy(list_of_goods,sk):#sum of all with discount
    total_sum = 0
    for good in list_of_goods:
        total_sum = total_sum + calc_new_price(good,sk)
    return total_sum

def print_check_doc(list_of_goods, sk):#print check with sum
    for good in list_of_goods:
        print(good.get("description"), "\t", calc_new_price(good, sk))
    print("Total = ", calculate_buy(list_of_goods,sk), "UAH")

good_1 = create_good("Chocolate", 218)#call func (create)
good_2 = create_good("Coffe", 512)
good_3 = create_good("Hamon", 2175)
good_4 = create_good("Bread", 31)
good_5 = create_good("Green tea", 82)#5 times: 5 goods need

list_of_goods = [good_1, good_2, good_3,good_4,good_5]#lst
sk = {
  500:5,
  1000:7,
  2000:10
}#dict of discounts(key - price, value - discount)
#print(list_of_goods)#list with goods (dictions)
print_check_doc(list_of_goods, sk)#call func of printing checkdoc

