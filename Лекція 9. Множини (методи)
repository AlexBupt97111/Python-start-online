s_1 = {1,3,5}
s_2 = {2,3,6}
print(len(s_1))
print(s_1.isdisjoint(s_2))# True if  no same (here False)
print(s_1.issubset(s_2))#i s all elem of s_1 is in s_2

#3
#False
#False

s_1 = {1,3,5}
s_2 = {2,3,6}
s_1.add(8)
print(s_1)
print(s_2)

#{8, 1, 3, 5} #- result of add
#{2, 3, 6}

s_1 = {1,3,5}
s_2 = {2,3,6}
s_2.remove(3) #del elem
print(s_2)

#{2, 6}
