d = {}
l = [1,2,3,6,3,1,7]
for i in l:
    c = d.get(i)#create counter (get elems)
    if c:# if elem find another time
        d[i] = c + 1#count + 1
    else:
        d[i] = 1#вперше
      
print(d)

########
{1: 2, 2: 1, 3: 2, 6: 1, 7: 1}

#########
import random
a = []
for i in range(10):
    a.append(random.randint(1, 5)) 

d = {}
for el in a:
    num = d.get(el)#create counter (get elems)
    if num is None:#
        d[el] = 1#
    else:
        d[el] = num + 1#if elem find another time
print(d)
